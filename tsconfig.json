{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": false,
        "skipLibCheck": true,
        "paths": {
            "@api/*": [
                "./api/*"
            ],
            "@database/*": [
                "./database/*"
            ],
            "@routes/*": [
                "./routes/*"
            ],
            "@utils/*": [
                "./utils/*"
            ],
            "@types": [
                "./types"
            ]
        },
    }
}